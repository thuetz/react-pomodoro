{
  "parser": "babel-eslint",
  "extends": [
	"airbnb"
  ],
  "plugins": [
	"babel"
  ],
  "rules": {
	"indent": [
	  2,
	  "tab"
	],
	"no-tabs": "off",
	"jsx-a11y/href-no-hash": "off",
	"import/no-named-as-default": "off",
	"react/jsx-indent": [
	  2,
	  "tab"
	],
	"react/jsx-indent-props": [
	  2,
	  "tab"
	],
	"comma-dangle": [
	  "error",
	  {
		"arrays": "always-multiline",
		"objects": "always-multiline",
		"imports": "always-multiline",
		"exports": "always-multiline",
		"functions": "never"
	  }
	]
  }
}
